{
  "name": "exilence-next-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.2",
    "@microsoft/signalr": "^3.0.1",
    "@microsoft/signalr-protocol-msgpack": "^3.0.0",
    "axios": "^0.19.0",
    "axios-observable": "^1.1.2",
    "clsx": "^1.0.4",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-react": "^7.15.0",
    "formik": "^1.5.8",
    "i18next": "^17.2.0",
    "i18next-xhr-backend": "^3.2.0",
    "localforage": "^1.7.3",
    "mobx": "^5.14.0",
    "mobx-logger": "^0.7.1",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.1.4",
    "mobx-utils": "^5.5.1",
    "moment": "^2.24.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-i18next": "^10.13.1",
    "react-image": "^2.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "rxjs": "^6.5.3",
    "rxjs-ratelimiter": "^1.0.1",
    "tslib": "^1.10.0",
    "typelevel-ts": "^0.3.5",
    "typescript": "3.6.3",
    "update-electron-app": "^1.5.0",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=never",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/electron-is-dev": "^1.1.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.1",
    "@types/react-router-dom": "^4.3.5",
    "@types/uuid": "^3.4.5",
    "@types/yup": "^0.26.24",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "com.exilence.exilence-next-app",
    "files": [
      "build/**/*"
    ],
    "extraFiles": [
      "public/i18n/**/*"
    ],
    "directories": {
      "buildResources": "src/assets/**/*"
    }
  }
}
