  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "eslint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier \"./**/*.{js,jsx,ts,tsx}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron-is-dev": "^1.1.1",
    "@types/jest": "26.0.15",
    "@types/node": "14.14.2",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-table": "^7.0.24",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "chokidar": "^3.5.1",
    "concurrently": "^5.0.0",
    "cross-env": "^7.0.2",
    "electron": "^12.0.7",
    "electron-builder": "^22.10.5",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.15.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "^2.1.2",
    "wait-on": "^5.3.0"
  },
  "build": {
    "productName": "Exilence Next",
    "appId": "com.exilence.exilence-next-app",
    "files": [
      "build/**/*",
      "public/icon.*"
    ],
    "extraFiles": [
      "public/i18n/**/*",
      "public/overlays/**/*",
      "public/background-tasks/**/*"
    ],
    "directories": {
      "buildResources": "src/assets/**/*"
    },
    "publish": {
      "provider": "github",
      "repo": "exilence-next",
      "owner": "viktorgullmark"
    },
    "win": {
      "icon": "build/icon.ico"
    }
  }
